{
  "name": "proactive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @opennextjs/cloudflare",
    "preview": "npm run pages:build && npx wrangler pages dev",
    "deploy": "npm run pages:build && npx wrangler pages deploy",
    "d1:create": "npx wrangler d1 create proactive-db",
    "d1:migrate": "npx wrangler d1 execute proactive-db --file=migrations/0001_init.sql",
    "d1:migrate:local": "npx wrangler d1 execute proactive-db --file=migrations/0001_init.sql --local"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.13",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.0",
    "tailwind-merge": "^2.2.1",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240903.0",
    "@opennextjs/cloudflare": "^0.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "esbuild": "^0.27.2",
    "eslint": "^8",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wrangler": "^3.80.0"
  }
}
